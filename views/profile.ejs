<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-colorpicker/2.5.3/css/bootstrap-colorpicker.css"
        rel="stylesheet">
    <script src="https://code.jquery.com/jquery-2.2.2.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.bundle.min.js"></script>
    <script
        src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-colorpicker/2.5.3/js/bootstrap-colorpicker.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/3.0.1/jspdf.umd.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/sql.js/1.5.0/sql-wasm.js"></script>

    <link rel="stylesheet" href="../public/styles/style.css">
</head>

<body>
    <header>
        <div class="container">
            <div class="container">
                <div class="row">
                    <div class="col-sm">
                        <p>Js Editor</p>
                    </div>
                    <div class="col-sm">
                        <a onclick="location.href='/'">Главная</a>
                    </div>
                    <div class="col-sm">
                        <a onclick="location.href='/login'">Профиль</a>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <main>
        <div class="container">
            <div class="row">
                <p>
                    Фотографии от <%=name %>
                </p>
            </div>
            <div id="photoContainer">
                <% photos.forEach(photo=> { %>
                    <div class="photo">
                        <img src="<%= photo %>" alt="Фотография">
                    </div>
                    <% }) %>
            </div>
        </div>
    </main>
    <footer>
        <p>hhhh</p>
    </footer>
</body>